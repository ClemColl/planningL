<%= simple_form_for(@calendar) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title, label: "Nom" %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, "Enregistrer" %>
    <%= link_to 'Retour', calendars_path, class: "btn btn-default" %>
  </div>
<% end %>
