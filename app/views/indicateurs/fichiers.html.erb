<%= form_tag indicateurs_production_results_path, { :multipart => true } do %>
    <%= label_tag :efficacite, "Fichier d'efficacite" %>
    <%= file_field_tag :efficacite %>

    <%= label_tag :utilisation, "Fichier d'utilisation" %>
    <%= file_field_tag :utilisation %>
    <br>
    <%= submit_tag 'Traiter', class: 'btn btn-default' %>
    <%= link_to 'Retour', indicateurs_path, class: 'btn btn-default'%>
<% end %>
