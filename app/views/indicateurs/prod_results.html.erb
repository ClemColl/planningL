
<% @equipes.each do |eq| %>
    <% analyzes = eq.analyzes.last(5)%>
    <% result_eff = {} %>
    <% obj_eff = {} %>

    <% result_util = {} %>
    <% obj_util = {} %>

    <% result_prod = {} %>
    <% obj_prod = {} %>
    

    <% analyzes.each do |an| %>
        <% result_eff[an.created_at.strftime("%d/%m")] = an.efficacite %>
        <% obj_eff[an.created_at.strftime("%d/%m")] = an.eff_obj %>

        <% result_util[an.created_at.strftime("%d/%m")] = an.utilisation %>
        <% obj_util[an.created_at.strftime("%d/%m")] = an.util_obj %>

        <% result_prod[an.created_at.strftime("%d/%m")] = (an.efficacite+an.utilisation)/2 %>
        <% obj_prod[an.created_at.strftime("%d/%m")] = (an.eff_obj + an.util_obj)/2 %>
    <% end %>

    <div style="display:inline-block;width:30%;">  
        <%= line_chart [{name: "Efficacité", data: result_eff}, {name: "Objectif", data: obj_eff}], legend: true, title: "Efficacite #{eq.re}", suffix: "%", min: 70, max: 150, download: true, colors: ["#5C98CE", "#E87C3B"] %>
    </div>

    <div style="display:inline-block;width:30%;">  
        <%= line_chart [{name: "Utilisation", data: result_util}, {name: "Objectif", data: obj_util}], legend: true, title: "Utilisation #{eq.re}", suffix: "%", min: 70, max: 150, download: true, colors: ["#5C98CE", "#E87C3B"] %>
    </div>

    <div style="display:inline-block;width:30%;">  
        <%= line_chart [{name: "Productivité", data: result_prod}, {name: "Objectif", data: obj_prod}], legend: true, title: "Productivité #{eq.re}", suffix: "%", min: 70, max: 150, download: true, colors: ["#5C98CE", "#E87C3B"] %>
    </div>

<% end %>


<div style="text-align:center;">
<%= link_to 'Retour aux indicateurs', indicateurs_path, class: 'btn btn-default'%>