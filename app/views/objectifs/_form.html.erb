<%= simple_form_for(@objectif) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%#= f.input :name %>
    <%= f.input :value %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, 'Enregistrer' %>
    <%= link_to 'Retour', objectifs_path, class: 'btn btn-default' %>
  </div>
<% end %>
