<p id="notice"><%= notice %></p>

<h1>Objectifs</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Value</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @objectifs.each do |objectif| %>
      <tr>
        <td><%= objectif.name %></td>
        <td><%= objectif.value %></td>
        <td><%= link_to 'Show', objectif %></td>
        <td><%= link_to 'Edit', edit_objectif_path(objectif) %></td>
        <td><%= link_to 'Destroy', objectif, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Objectif', new_objectif_path %>
