<p id="notice"><%= notice %></p>

<h1>Rapports</h1>

<table>
  <thead>
    <tr>
      <th>Cas</th>
      <th>Pdp</th>
      <th>Cta</th>
      <th>Dav</th>
      <th>Dsa</th>
      <th>Machine</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @rapports.each do |rapport| %>
      <tr>
        <td><%= rapport.cas %></td>
        <td><%= rapport.pdp %></td>
        <td><%= rapport.cta %></td>
        <td><%= rapport.dav %></td>
        <td><%= rapport.dsa %></td>
        <td><%= rapport.machine.name %></td>
        <%#= link_to 'Show', rapport %>
        <td><%= link_to 'Modifier', edit_rapport_path(rapport) %></td>
        <%#= link_to 'Destroy', rapport, method: :delete, data: { confirm: 'Are you sure?' } %>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Rapport', new_rapport_path %>
