<%= simple_form_for(@rotation_stock) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :rotation %>
    <%= f.input :objectif %>
    <%#= f.input :mois %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, 'Enregistrer' %>
    <%= link_to 'Retour', indicateurs_path, class: 'btn btn-default' %>
  </div>
<% end %>
